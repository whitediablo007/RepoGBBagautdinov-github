<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
      fx:controller="AuthController" spacing="4" prefHeight="150.0" prefWidth="300.0">
    <Label text="Логин"/>
    <TextField fx:id="loginTF" promptText="Логин"/>
    <Label text="Пароль"/>
    <PasswordField fx:id="passwordTF" promptText="Пароль"/>
    <Button text="Войти" onAction="#auth"/>
    <padding>
        <Insets bottom="5.0" left="5.0" top="5.0" right="5.0"/>
    </padding>
</VBox>

